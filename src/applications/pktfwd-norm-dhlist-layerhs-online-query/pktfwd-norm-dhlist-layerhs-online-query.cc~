    strlist ("pReturn_attr1", "pReturn_attr2", "pReturn_attr3"));

  result = result->Project (
    RECORDPROV,
    strlist ("pReturn_attr1",
      "pReturn_attr2",
      "pReturn_attr3",
      "pReturn_attr4"),
    strlist ("recordProv_attr1",
      "recordProv_attr2",
      "recordProv_attr3",
      "recordProv_attr4"));

  SendLocal (result);
}

