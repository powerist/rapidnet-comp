## -*- Mode: python; py-indent-offset: 4; indent-tabs-mode: nil; coding: utf-8; -*-

def build(bld):
    module = bld.create_ns3_module('dns-prov-dist-hlist-rmitm', ['internet-stack'])
    module.source = [
        'dns-prov-dist-hlist-rmitm.cc',
        ]
    headers = bld.new_task_gen('ns3header')
    headers.module = 'dns-prov-dist-hlist-rmitm'
    headers.source = [
        'dns-prov-dist-hlist-rmitm.h',
        'dns-prov-dist-hlist-rmitm-helper.h',
        ]
